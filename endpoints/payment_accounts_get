output_file='PaymentAccounts.json'
endpoint_destination="/api/v1/payment-accounts"
method='GET'
data=""
function on_success() {

	bank_account_id=$(cat $3/$output_file | jq -r 'first(.data[] | select(.type=="bank-accounts")) | .id')
	debit_card_id=$(cat $3/$output_file | jq -r 'first(.data[] | select(.type=="debit-cards")) | .id')
}
